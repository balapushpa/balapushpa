import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int t,q;
        int p=s.nextInt();
        if(p<100 || p>200)
        {
            System.out.println("INVALID INPUT");
        }
        else
        {
            t=p/4;
            q=p%4;
            
         System.out.printf("%d\n%d\n%d\n%d\n",t,t,t,t+q);   
        }
    }
}
