import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        
        int s1=s.nextInt();
        int l=s.nextInt();
        int b=s.nextInt();
      float r=s.nextFloat();
        int a=s1*s1;
        System.out.println(a);
        int r1=l*b;
        System.out.println(r1);
        float f=(float)3.14*r*r;
        System.out.printf("%.2f",f);
    }
}
