import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
            //int s1,t;
        int s1=s.nextInt();
        int t=s.nextInt();
        int a,b;
        a=s1/t;
        b=s1%t;
        System.out.println("The number of students in each team is "+a+" and left out is "+b);
    }
}
